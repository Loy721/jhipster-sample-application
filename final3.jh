entity File {
    topic String
    content String
}

entity MyTest {
	name String
    questions String
}

entity Question {
	content String
}

entity UserTest {
    grade Integer
}

entity MyUser {
	name String
    surname String
}

relationship OneToMany {
	File{myTests} to MyTest{file}
    MyTest{questions} to Question{myTest}
    MyUser{myTests} to UserTest{myUser}
    MyTest{myUsers} to UserTest{myTest}
}
