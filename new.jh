entity File {
    topic String
    content String
}

entity Test {
    name String
    // Ссылка на сущность Question
}

entity Question {
    content String
}

entity UserTest {
    grade Integer
}

entity MyUser {
    name String
    surname String
}

relationship OneToMany {
    File{tests} to Test{file}
    Test{questions} to Question{test}
    MyUser{tests} to UserTest{myUser}
    Test{users} to UserTest{test}
}

