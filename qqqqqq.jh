
entity File{
    topic String
    content String
}

entity Test {
	name String
    questions String
}

entity Question {
	content String
}

entity UserTest {
    grade Integer
}

entity MyUser {
	name String
    surname String
}

relationship ManyToOne {
	Test{file} to File{tests}
}

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl except Files, Tests, Questions, MyUsers, UserTest
